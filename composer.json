{
    "name": "geo-io/wkt-parser",
    "description": "Well-known text (WKT) Parser.",
    "keywords": [
        "geo",
        "io",
        "geometry",
        "wkt",
        "parser"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Sorgalla",
            "email": "jsorgalla@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "geo-io/interface": "^2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.1",
        "vimeo/psalm": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "GeoIO\\WKT\\Parser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeoIO\\WKT\\Parser\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
